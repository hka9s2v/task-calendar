{
  "name": "task-calendar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config=config/jest/jest.config.js",
    "test:watch": "jest --config=config/jest/jest.config.js --watch",
    "test:coverage": "jest --config=config/jest/jest.config.js --coverage",
    "test:ci": "jest --config=config/jest/jest.config.js --ci --coverage --watchAll=false",
    "test:verbose": "jest --config=config/jest/jest.config.js --verbose",
    "test:silent": "jest --config=config/jest/jest.config.js --silent",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then prisma generate --schema=prisma/schema.production.prisma; else prisma generate; fi",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:deploy:neon": "prisma db push --schema=prisma/schema.production.prisma",
    "db:generate:neon": "prisma generate --schema=prisma/schema.production.prisma",
    "db:studio:neon": "prisma studio --schema=prisma/schema.production.prisma"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^3.0.2",
    "next": "^14.1.0",
    "next-auth": "^4.24.11",
    "pg": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.35",
    "prisma": "^5.10.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.3.3"
  }
}
